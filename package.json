{
  "name": "saas-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 4002",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start --port 4002",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "playwright:update": "playwright install && playwright install-deps",
    "test:e2e": "playwright test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --env=jsdom",
    "build-ci": "next build"
  },
  "dependencies": {
    "@boxyhq/metrics": "0.2.6",
    "@boxyhq/react-ui": "3.3.25",
    "@boxyhq/saml-jackson": "1.15.6",
    "@heroicons/react": "2.1.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.7.1",
    "@react-email/components": "0.0.12",
    "@retracedhq/logs-viewer": "2.7.0",
    "@retracedhq/retraced": "0.7.6",
    "@sentry/nextjs": "7.92.0",
    "@tailwindcss/typography": "0.5.10",
    "bcryptjs": "2.4.3",
    "classnames": "2.5.1",
    "cookies-next": "4.1.0",
    "crypto-js": "4.2.0",
    "formik": "2.4.5",
    "i18next": "22.5.1",
    "micromatch": "4.0.5",
    "mixpanel-browser": "2.48.1",
    "next": "14.0.4",
    "next-auth": "4.24.5",
    "next-i18next": "13.3.0",
    "nodemailer": "6.9.8",
    "react": "18.2.0",
    "react-daisyui": "3.1.2",
    "react-dom": "18.2.0",
    "react-email": "1.10.0",
    "react-google-recaptcha": "3.1.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "12.3.1",
    "sharp": "0.32.6",
    "slack-notify": "2.0.7",
    "svix": "1.15.0",
    "swr": "2.2.4",
    "yup": "1.3.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@testing-library/jest-dom": "6.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/bcryptjs": "2.4.6",
    "@types/crypto-js": "4.2.1",
    "@types/jest": "29.5.11",
    "@types/micromatch": "4.0.6",
    "@types/mixpanel-browser": "2.48.1",
    "@types/node": "20.10.7",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18.2.47",
    "@typescript-eslint/eslint-plugin": "6.18.0",
    "@typescript-eslint/parser": "6.18.0",
    "autoprefixer": "10.4.16",
    "daisyui": "2.52.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.33.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
